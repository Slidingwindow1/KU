q20: last and second last character in string with space : apple return "e l"
        string a=" apple";
        int len= a.Length;
        char b= a[len-1];
        char c= a[len-2];
        string o= b.ToString()+ " "+c.ToString();
        Console.WriteLine(b+" "+c);
        Console.WriteLine("Sum of x + y = "+ o);
