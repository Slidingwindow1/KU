int count = 0;
 int n = A.Length;
        // The three loops select three
        // different values from array
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
 
                // The innermost loop checks for
                // the triangle property
                for (int k = j + 1; k < n; k++)
 
                    // Sum of two sides is greater
                    // than the third
                    if (A[i] + A[j] > A[k]
                        && A[i] + A[k] > A[j]
                        && A[k] + A[j] > A[i])
                        count++;
            }
        }
        return count;
